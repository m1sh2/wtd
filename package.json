{
  "name": "wtd",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "start": "npm run server:prod",
    "watch": "concurrently \"npm run server:dev\" \"npm run build:dev\"",
    "watch:dev": "npm run build:dev -- --watch",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config webpack.config.dev.js --progress --profile --display-cached",
    "prebuild:prod": "npm run clean",
    "build:prod": "ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'public/js/app/**/*.js' 'public/js/app/**/*.js.map' '!public/js/app/bundle.js' '!public/js/app/*.chunk.js' 'assets/app/**/*.ngfactory.ts' 'assets/app/**/*.shim.ts'",
    "server:dev": "PORT=8000 NODE_ENV=development node ./bin/www",
    "server:prod": "PORT=8000 NODE_ENV=production node ./bin/www",
    "server:static": "light-server -s . -p 8000",
    "rimraf": "rimraf",
    "clean": "npm run rimraf -- dist",
    "db:dev": "./db/bin/mongod --dbpath ./db"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "0.6.3",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/platform-server": "2.0.1",
    "@angular/router": "3.0.1",
    "@angular/upgrade": "2.0.1",
    "@types/jquery": "^2.0.34",
    "@types/lodash": "^4.14.41",
    "bcryptjs": "^2.3.0",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "hbs": "~3.1.0",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.17.2",
    "mongoose": "^4.6.7",
    "mongoose-unique-validator": "^1.0.2",
    "morgan": "~1.6.1",
    "phaser": "^2.6.2",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "~2.3.0",
    "uuid": "^3.0.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.45",
    "angular2-router-loader": "^0.3.2",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "del-cli": "^0.2.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "light-server": "^1.1.7",
    "ng2-logger": "^0.1.21",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "typescript": "^2.1.1",
    "webpack": "^2.1.0-beta.21",
    "webpack-build-notifier": "^0.1.12",
    "webpack-merge": "^0.14.1"
  }
}